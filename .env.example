# Agentic Internet Environment Variables
# Copy this file to .env and fill in your API keys

# HuggingFace API Token (for using HuggingFace models)
# Get your token from: https://huggingface.co/settings/tokens
HUGGINGFACE_TOKEN=

# OpenAI API Key (for GPT models)
# Get your key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=

# Anthropic API Key (for Claude models)
# Get your key from: https://console.anthropic.com/settings/keys
ANTHROPIC_API_KEY=

# SerpAPI Key (for advanced web search capabilities)
# Get your key from: https://serpapi.com/manage-api-key
SERPAPI_API_KEY=

# OpenRouter API Key (for multi-model access)
# Get your key from: https://openrouter.ai/keys
OPENROUTER_API_KEY=


BROWSER_USE_API_KEY=

# Optional: Custom model settings
# MODEL_NAME=meta-llama/Llama-3.3-70B-Instruct
# MODEL_TEMPERATURE=0.7
# MAX_TOKENS=2048
